@inject PlacesCache cache

<RadzenPanel class="flex-column">
    <RadzenImage Path="images/header-logo.png" Style="height:3rem" />
    <RadzenIcon Icon="room" />
    <RadzenTextBox Placeholder="Search..." Value="@words" Disabled="true" />
    <RadzenButton Text="Search" Click="Search" Disabled="true" />
</RadzenPanel>


@code {

    string words;

    public async void Search(EventArgs e)
    {
        cache.SearchAsync(words);
        StateHasChanged();

    }
}
